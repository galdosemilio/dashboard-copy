{
  "name": "dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "node --max-old-space-size=6144 node_modules/.bin/nx",
    "start": "node --max-old-space-size=6144 node_modules/.bin/ng serve",
    "start:admin": "node --max-old-space-size=6144 node_modules/.bin/ng serve admin --port 4200",
    "start:provider": "node --max-old-space-size=6144 node_modules/.bin/ng serve provider --port 4201 -c standalone",
    "start:provider-noSourceMap": "node --max-old-space-size=6144 node_modules/.bin/ng serve provider --sourceMap=false --port 4201 -c standalone",
    "test:admin": "yarn e2e admin-e2e --watch",
    "test:provider": "yarn e2e provider-e2e --watch",
    "build": "node --max-old-space-size=6144 node_modules/.bin/ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "node --max-old-space-size=6144 node_modules/.bin/nx e2e",
    "extract": "yarn extract:admin && yarn extract:provider && yarn extract:cleanup",
    "extract:cleanup": "node_modules/.bin/ts-node ./tools/i18n",
    "extract:admin": "node_modules/.bin/ngx-translate-extract -i ./apps/admin/src/app ./libs/layout/src/lib ./libs/backend/src/lib ./libs/common/src/lib -o ./apps/admin/src/i18n/base/*.json -c -s -f namespaced-json -m _ --fi '  '",
    "extract:provider": "node_modules/.bin/ngx-translate-extract -i ./apps/provider/src/app ./libs/common/src/lib/components/form/fields/date-input -o ./apps/provider/src/assets/i18n/base/*.json -c -s -f namespaced-json -m _ --fi '  '",
    "extract:common": "node_modules/.bin/ngx-translate-extract --input ./libs/common/src/lib --output ./libs/common/src/i18n/*.json --clean --sort --format namespaced-json --marker _ --fi '  '",
    "extract:layout": "node_modules/.bin/ngx-translate-extract --input ./libs/layout/src/lib --output ./libs/layout/src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --fi '  '",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "node --max-old-space-size=6144 node_modules/.bin/nx affected:build",
    "affected:e2e": "node --max-old-space-size=6144 node_modules/.bin/nx affected:e2e",
    "affected:test": "node --max-old-space-size=6144 node_modules/.bin/nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "cy-serve:provider": "concurrently \"yarn start:provider --live-reload=false --watch=false\" \"sleep 240s && yarn cy:provider\" --kill-others --success=first",
    "cy-serve:admin": "concurrently \"yarn start:admin --live-reload=false --watch=false\" \"sleep 240s && yarn cy:admin\" --kill-others --success=first",
    "cy:provider": "node --max-old-space-size=6144 ./node_modules/.bin/cypress run --project './apps/provider-e2e' --config-file './cypress.ci.json' --headless --record --parallel --key $CYPRESS_PROVIDER_RECORD_KEY",
    "cy:admin": "node --max-old-space-size=6144 ./node_modules/.bin/cypress run --project './apps/admin-e2e' --config-file './cypress.json' --headless --record --parallel --key $CYPRESS_ADMIN_RECORD_KEY"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.3",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.1.3",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/router": "10.1.3",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/nx": "^7.8.7",
    "@types/lodash": "^4.14.161",
    "ang-jsoneditor": "^1.10.2",
    "angularx-qrcode": "^10.0.10",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "jsoneditor": "^9.1.1",
    "linkifyjs": "^2.1.9",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "ng2-charts": "^2.4.2",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-color-picker": "^10.0.1",
    "ngx-cookie-service": "^10.0.1",
    "ngx-device-detector": "^2.0.0",
    "ngx-moment": "^5.0.0",
    "pdfmake": "^0.1.68",
    "rxjs": "~6.6.3",
    "socket.io": "^2.3.0",
    "tinycolor2": "^1.4.1",
    "twilio-video": "^2.7.3",
    "zone.js": "^0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.3",
    "@angular/language-service": "10.1.3",
    "@biesbjerg/ngx-translate-extract": "~2.3.4",
    "@nrwl/angular": "^10.2.1",
    "@nrwl/cypress": "10.2.1",
    "@nrwl/jest": "10.2.1",
    "@nrwl/workspace": "10.2.1",
    "@types/jest": "26.0.14",
    "@types/node": "^14.14.6",
    "codelyzer": "~6.0.0",
    "concurrently": "^5.3.0",
    "cypress": "^5.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.9.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^4.3.0",
    "jest": "26.4.2",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "^10.1.2",
    "prettier": "2.1.2",
    "terser": "^5.3.8",
    "ts-jest": "26.4.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
