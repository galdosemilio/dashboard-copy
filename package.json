{
  "name": "dashboard",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "CoachCare",
  "repository": "github:coachcare/dashboard",
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "issuePrefixes": [
              "FRON-",
              "IDEA-"
            ],
            "issueUrlFormat": "https://selvera.atlassian.net/browse/{prefix}{id}"
          },
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ],
      "body-max-line-length": [
        2,
        "always",
        200
      ]
    }
  },
  "scripts": {
    "ng": "nx",
    "nx": "node --max-old-space-size=6144 node_modules/.bin/nx",
    "start": "node --max-old-space-size=6144 node_modules/.bin/nx serve",
    "start:admin": "node --max-old-space-size=6144 node_modules/.bin/nx serve admin --port 4200",
    "start:provider": "node --max-old-space-size=6144 node_modules/.bin/nx serve provider --port 4201",
    "start:provider-noSourceMap": "node --max-old-space-size=6144 node_modules/.bin/nx serve provider --sourceMap=false --port 4201 -c standalone",
    "start:chart": "nx serve measurement-charts --port 4202 --host 0.0.0.0",
    "test:admin": "nx e2e admin-e2e --watch",
    "test:provider": "nx e2e provider-e2e -c e2e --watch",
    "test:chart": "nx e2e measurement-charts-e2e --watch",
    "build": "node --max-old-space-size=6144 node_modules/.bin/nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "node --max-old-space-size=6144 node_modules/.bin/nx e2e",
    "extract": "yarn extract:admin && yarn extract:provider && yarn extract:storefront && yarn extract:cleanup",
    "extract:cleanup": "node_modules/.bin/ts-node ./tools/i18n",
    "extract:admin": "node_modules/.bin/ngx-translate-extract -i ./apps/admin/src/app ./libs/layout/src/lib ./libs/backend/src/lib ./libs/common/src/lib -o ./apps/admin/src/i18n/base/*.json -c -s -f namespaced-json -m _ --fi '  '",
    "extract:provider": "node_modules/.bin/ngx-translate-extract -i ./apps/provider/src/app ./libs/common/src/lib/components/form/fields/date-input ./libs/common/src/lib/components/utilities/datasource-overlay ./libs/common/src/lib/components/pages ./libs/backend/src/lib/data/organization -o ./apps/provider/src/assets/i18n/base/*.json -c -s -f namespaced-json -m _ --fi '  '",
    "extract:common": "node_modules/.bin/ngx-translate-extract --input ./libs/common/src/lib --output ./libs/common/src/i18n/*.json --clean --sort --format namespaced-json --marker _ --fi '  '",
    "extract:layout": "node_modules/.bin/ngx-translate-extract --input ./libs/layout/src/lib --output ./libs/layout/src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --fi '  '",
    "extract:storefront": "node_modules/.bin/ngx-translate-extract --input ./libs/storefront/src/lib --output ./libs/storefront/src/assets/i18n/base/*.json --clean --sort --format namespaced-json --marker _ --fi '  '",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "node --max-old-space-size=6144 node_modules/.bin/nx affected:build",
    "affected:e2e": "node --max-old-space-size=6144 node_modules/.bin/nx affected:e2e",
    "affected:test": "node --max-old-space-size=6144 node_modules/.bin/nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "cy-serve:provider": "yarn cy:provider",
    "cy-serve:admin": "yarn cy:admin",
    "cy:provider": "node --max-old-space-size=6144 ./node_modules/.bin/nx e2e provider-e2e -c e2e --headless --record --parallel --key $CYPRESS_PROVIDER_RECORD_KEY -- --parallel",
    "cy:admin": "node --max-old-space-size=6144 ./node_modules/.bin/nx e2e admin-e2e --headless --record --parallel --key $CYPRESS_ADMIN_RECORD_KEY -- --parallel",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^13.1.0",
    "@angular/animations": "13.1.3",
    "@angular/cdk": "13.1.3",
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/flex-layout": "13.0.0-beta.36",
    "@angular/forms": "13.1.3",
    "@angular/material": "13.1.3",
    "@angular/platform-browser": "13.1.3",
    "@angular/platform-browser-dynamic": "13.1.3",
    "@angular/router": "13.1.3",
    "@coachcare/sdk": "^24.8.0",
    "@kolkov/ngx-gallery": "^2.0.1",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngrx/effects": "13.0.2",
    "@ngrx/router-store": "13.0.2",
    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@spree/storefront-api-v2-sdk": "^4.13.1",
    "@stripe/stripe-js": "^1.23.0",
    "@twilio/video-processors": "^1.0.2",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.27.0",
    "ang-jsoneditor": "^1.10.5",
    "angularx-qrcode": "^13.0.3",
    "chart.js": "^2.9.3",
    "core-js": "^3.21.1",
    "document-register-element": "1.13.1",
    "jsoneditor": "^9.5.7",
    "lightweight-charts": "^3.5.0",
    "linkify-html": "^3.0.5",
    "linkifyjs": "^3.0.5",
    "lodash": "^4.17.21",
    "luxon": "^1.28.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "ng2-charts": "^2.4.2",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-color-picker": "^12.0.1",
    "ngx-cookie-service": "^13.1.2",
    "ngx-device-detector": "^3.0.0",
    "ngx-moment": "^6.0.2",
    "ngx-stripe": "^13.0.0",
    "owasp-password-strength-test": "^1.3.0",
    "pdfmake": "^0.2.7",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~7.5.6",
    "socket.io": "^4.5.4",
    "tinycolor2": "^1.4.1",
    "tslib": "^2.0.0",
    "twilio-video": "^2.20.0",
    "url-parse": "^1.5.10",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.4",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.1.4",
    "@angular/compiler-cli": "13.1.3",
    "@angular/language-service": "13.1.3",
    "@biesbjerg/ngx-translate-extract": "~2.3.4",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^12.1.4",
    "@nrwl/angular": "13.4.6",
    "@nrwl/cli": "13.4.6",
    "@nrwl/cypress": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/tao": "13.4.6",
    "@nrwl/web": "13.8.3",
    "@nrwl/workspace": "13.4.6",
    "@octokit/core": "^3.5.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@testing-library/angular": "^11.0.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/flat": "^5.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "concurrently": "^5.3.0",
    "cypress": "^7.0.0",
    "dotenv": "10.0.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "7",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "ng-mocks": "^14.3.1",
    "ng-packagr": "13.1.3",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.6.2",
    "semantic-release": "^19.0.3",
    "terser": "^5.14.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.6.0",
    "typescript": "4.4.4"
  }
}
