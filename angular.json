{
  "version": 1,
  "projects": {
    "provider": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/provider",
      "sourceRoot": "apps/provider/src",
      "prefix": "provider",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/provider",
            "index": "apps/provider/src/index.html",
            "main": "apps/provider/src/main.ts",
            "polyfills": "apps/provider/src/polyfills.ts",
            "tsConfig": "apps/provider/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/provider/src/favicon.ico",
              "apps/provider/src/assets"
            ],
            "styles": [
              "apps/provider/src/styles.scss",
              "apps/provider/src/styles/app.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/provider/src/styles",
                "libs/common/src/lib/styles",
                "libs/common/src/lib",
                "libs/datepicker/src/lib"
              ]
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "lodash",
              "selvera-api",
              "qrcode",
              "axios",
              "tinycolor2",
              "url-parse",
              "qs",
              "moment-timezone",
              "moment",
              "linkifyjs",
              "twilio-video",
              "ngrx-store-freeze"
            ]
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "apps/provider/src/environments/environment.ts",
                  "with": "apps/provider/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "apps/provider/src/environments/environment.ts",
                  "with": "apps/provider/src/environments/environment.test.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "standalone": {
              "fileReplacements": [
                {
                  "replace": "apps/provider/src/environments/environment.ts",
                  "with": "apps/provider/src/environments/environment.standalone.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "provider:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "provider:build:production"
            },
            "test": {
              "browserTarget": "provider:build:test"
            },
            "standalone": {
              "browserTarget": "provider:build:standalone"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "provider:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/provider/tsconfig.app.json",
              "apps/provider/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/provider/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/provider/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "provider-e2e": {
      "root": "apps/provider-e2e",
      "sourceRoot": "apps/provider-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/provider-e2e/cypress.json",
            "tsConfig": "apps/provider-e2e/tsconfig.e2e.json",
            "devServerTarget": "provider:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "provider:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/provider-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/provider-e2e/**/*"]
          }
        }
      }
    },
